{
  "name": "aurionstaff",
  "version": "1.0.0",
  "description": "Bot Discord pour le staff mineaurion",
  "main": "index.js",
  "lint-staged": {
    "*.{js,ts,html,md,scss,json}": "npm run format:write",
    "*{!json}": "npm run lint"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "build": "tsc && cp -R src/api/templates build/api/",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js",
    "test": "npm run lint && npm run format",
    "format": "prettier \"./src/**/*.{ts,html,md,scss,json}\" -c",
    "format:write": "prettier \"./src/**/*.{ts,html,md,scss,json}\" --write",
    "lint": "eslint \"{src,libs}/**/*.ts\"",
    "lint:fix": "eslint \"{src,libs}/**/*.ts\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mineaurion/AurionStaff.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mineaurion/AurionStaff/issues"
  },
  "homepage": "https://github.com/Mineaurion/AurionStaff#readme",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@discordx/importer": "^1.1.2",
    "@discordx/koa": "^1.0.8",
    "@discordx/pagination": "^1.2.0",
    "@koa/router": "^10.1.1",
    "discord.js": "^13.6.0",
    "discordx": "^9.3.1",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "multicraft-api-node": "^2.4.0",
    "mustache": "^4.2.0",
    "node-fetch": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/mustache": "^4.1.2",
    "@types/node": "^17.0.18",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "discord-api-types": "^0.27.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
